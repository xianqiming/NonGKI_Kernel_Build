CONFIG_ENV=Profiles/ruby_alphadroid_a15.env

DEVICE_NAME=redmi_note12pro
DEVICE_CODENAME=ruby

CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu-
EXTRA_CMDS:LD=ld.lld AR=llvm-ar NM=llvm-nm OBJCOPY=llvm-objcopy OBJDUMP=llvm-objdump READELF=llvm-readelf OBJSIZE=llvm-size STRIP=llvm-strip LLVM=1 LLVM_IAS=1

KERNEL_SOURCE=https://github.com/qiangua2460/kernel_xiaomi_mt6877.git
