##CONFIG_ENV - Profiles/设备代号_ROM名称.env
CONFIG_ENV=Profiles/gauguin_LineageOS_A15.env

##DEVICE_NAME - 设备全称，格式：设备品牌_型号_地区
##DEVICE_CODENAME - 设备代号
DEVICE_NAME=Redmi_note_9_pro_CN
DEVICE_CODENAME=gauguin

##CUSTOM_CMDS - 通常用于指明所用编译器/备用编译器
##EXTRA_CMDS - 通常用于编译器所需的自定义参数
##我认为这个地方可以不做更改
CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu-
EXTRA_CMDS:LD=ld.lld LLVM=1 LLVM_IAS=1

##KERNEL_SOURCE - 内核源码所在之处
##KERNEL_BRANCH - 内核源码所需分支
KERNEL_SOURCE=https://github.com/xianqiming/android_kernel_xiaomi_gauguin
KERNEL_BRANCH=lineage-22.1

##CLANG_SOURCE - Clang所在之处，但支持git、tar.gz、tar.xz
##CLANG_BRANCH - Clang所需分支，但前提是git
#CLANG_SOURCE=https://github.com/llvm/llvm-project/releases/download/llvmorg-19.1.7/LLVM-19.1.7-Linux-X64.tar.xz
CLANG_SOURCE=
CLANG_BRANCH=

GCC_GNU=false
GCC_64_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9
GCC_64_BRANCH=android12L-release
GCC_32_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/x86/x86_64-linux-android-4.9
GCC_32_BRANCH=android12L-release

DEFCONFIG_SOURCE=
DEFCONFIG_NAME=gauguin_defconfig
DEFCONFIG_ORIGIN_DEFCONFIG=

KERNELSU_SOURCE=https://raw.githubusercontent.com/tiann/KernelSU/refs/heads/main/kernel/setup.sh
KERNELSU_BRANCH=v0.9.5
KERNELSU_NAME=KernelSU

SUSFS_ENABLE=true
SUSFS_FIXED=false

AK3_SOURCE=https://github.com/osm0sis/AnyKernel3.git
AK3_BRANCH=master

BOOT_SOURCE=https://mirrorbits.lineageos.org/full/gauguin/20250319/boot.img

LXC_ENABLE=false

HAVE_NO_DTBO=false

ROM_TEXT=lineage-22.1
